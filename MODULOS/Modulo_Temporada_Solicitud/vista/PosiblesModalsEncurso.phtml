    <script type="text/javascript">
    $(document).ready(function(){

          $("#guardar_entregables_estudiante").click(function(){
            
            var idsEntregables = Array(); 

            $(".toggle-demo-check:checked").each( function(index , values){

                algo  = $(this).parent(); 
               
                thing = algo.parent();
               
                padre = thing.parent();
                
                id_Entregable = padre.find('label[class=id_entregableAsiiiiiiiiii]');
                
                valorAdd      = $(this).parent().parent().find('.form-control').val();

                veredicte     = false ; if (valorAdd.length>0) veredicte = true ;
                
                idsEntregables[index]=  
                {
                  id_entregable       : id_Entregable.html(),
                  Verificar_asignacion: veredicte , 
                  ValorEntregable     : valorAdd
                };
                
            });
             
              var codigo_estudiante = $(".codigo_estudiante_para_entregableAsignacion").html();
             
              if(idsEntregables.length>0)  AsignarEstosEntregables_a_estudiantes( idsEntregables, codigo_estudiante);
             
              else Mensajes( ' Seleccione uno o varios de la lista ' );
            
          });
        });

    </script>